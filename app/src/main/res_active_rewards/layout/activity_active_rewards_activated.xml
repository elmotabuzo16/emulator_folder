<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <android.support.design.widget.CoordinatorLayout
        android:id="@+id/root_coordinator_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/activity_active_rewards_activated_activiy"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/onboarding_improve_your_health"
            tools:context="com.vitalityactive.va.activerewards.ActiveRewardsActivatedActivity">

            <include
                android:id="@+id/onboarding_circles"
                layout="@layout/onboarding_circles"/>

            <include layout="@layout/onboarding_content"
                />

        </RelativeLayout>
    </android.support.design.widget.CoordinatorLayout>
</layout>
