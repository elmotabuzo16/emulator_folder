<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/ar_padding_regular"
        android:layout_marginEnd="@dimen/card_horizontal_margin_medium"
        android:layout_marginLeft="@dimen/card_horizontal_margin_medium"
        android:layout_marginRight="@dimen/card_horizontal_margin_medium"
        android:layout_marginStart="@dimen/card_horizontal_margin_medium"
        android:layout_marginTop="@dimen/card_vertical_margin_medium"
        android:text="@string/AR.landing.activity_cell_title_711"
        android:textAppearance="@style/MaterialLightBody2SectionHeaders" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/AR.history.no_activity_heading_title_694"
        android:textAppearance="@style/MaterialLightSubhead" />
</LinearLayout>