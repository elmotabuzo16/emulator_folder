<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/card_vertical_margin_medium"
        android:layout_marginEnd="@dimen/card_horizontal_margin_medium"
        android:layout_marginLeft="@dimen/card_horizontal_margin_medium"
        android:layout_marginRight="@dimen/card_horizontal_margin_medium"
        android:layout_marginStart="@dimen/card_horizontal_margin_medium"
        android:layout_marginTop="@dimen/card_vertical_margin_medium"
        android:text="@string/AR.landing.this_weeks_activity_section_header_title_660"
        android:textAppearance="@style/MaterialLightBody2SectionHeaders" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/AR.landing.no_activity_cell_title_678"
        android:textAppearance="@style/MaterialLightSubhead" />
</LinearLayout>